);else if(\"bottom\"===position)y=me.top+hTickAndPadding,textAlign=me._getXAxisLabelAlignment();else if(\"left\"===position){const ret=me._getYAxisLabelAlignment(tl);textAlign=ret.textAlign,x=ret.x}else if(\"right\"===position){const ret=me._getYAxisLabelAlignment(tl);textAlign=ret.textAlign,x=ret.x}else if(\"x\"===axis){if(\"center\"===position)y=(chartArea.top+chartArea.bottom)/2+tickAndPadding;else if(helpers_segment_isObject(position)){const positionAxisID=Object.keys(position)[0];y=me.chart.scales[positionAxisID].getPixelForValue(position[positionAxisID])+tickAndPadding}textAlign=me._getXAxisLabelAlignment()}else if(\"y\"===axis){if(\"center\"===position)x=(chartArea.left+chartArea.right)/2-tickAndPadding;else if(helpers_segment_isObject(position)){const positionAxisID=Object.keys(position)[0];x=me.chart.scales[positionAxisID].getPixelForValue(position[positionAxisID])}textAlign=me._getYAxisLabelAlignment(tl).textAlign}\"y\"===axis&&(\"start\"===align?textBaseline=\"top\":\"end\"===align&&(textBaseline=\"bottom\"));const labelSizes=me._getLabelSizes();for(i=0,ilen=ticks.length;i<ilen;++i){tick=ticks[i],label=tick.label;const optsAtIndex=optionTicks.setContext(me.getContext(i));pixel=me.getPixelForTick(i)+optionTicks.labelOffset,font=me._resolveTickFontOptions(i),lineHeight=font.lineHeight,lineCount=helpers_segment_isArray(label)?label.length:1;const halfCount=lineCount/2,color=optsAtIndex.color,strokeColor=optsAtIndex.textStrokeColor,strokeWidth=optsAtIndex.textStrokeWidth;let backdrop;if(isHorizontal?(x=pixel,textOffset=\"top\"===position?\"near\"===crossAlign||0!==rotation?-lineCount*lineHeight+lineHeight/2:\"center\"===crossAlign?-labelSizes.highest.height/2-halfCount*lineHeight+lineHeight:lineHeight/2-labelSizes.highest.height:\"near\"===crossAlign||0!==rotation?lineHeight/2:\"center\"===crossAlign?labelSizes.highest.height/2-halfCount*lineHeight:labelSizes.highest.height-lineCount*lineHeight,mirror&&(textOffset*=-1)):(y=pixel,textOffset=(1-lineCount)*lineH